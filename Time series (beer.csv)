   "cell_type": "markdown",
   "id": "a2f7ab6c",
   "metadata": {},
   "source": [
    "# calculating Rolling average"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "ea436d94",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['MA2'] = df['RPI:Weights (parts per 1000) - beer off sales'].rolling(window=2).mean()\n",
    "df['MA4'] = df['RPI:Weights (parts per 1000) - beer off sales'].rolling(window=4).mean()\n",
    "df['MA6'] = df['RPI:Weights (parts per 1000) - beer off sales'].rolling(window=6).mean()"
   ]
  },
